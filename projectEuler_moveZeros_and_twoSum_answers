two sum 

nums1 = [1, 5, 6, 9, 15]
target = 20

def two_sum(arr1, arr2,  target)
    arr.each_with_index do |num1, index1|
        arr.each_with_index do |num2, index2|
            if index2 > index1 && num1 + num2 == target
                return "{index1: #{index1}, index2: #{index2}}"
            end
        end
    end
end

p two_sum(nums, target)
